{
  "name": "orotter-bootstrap",
  "version": "0.0.2",
  "description": "A CSS toolset for building oRotter application.",
  "scripts": {
    "build": "npm run build:sass && npm run build:sass:min",
    "build:sass": "gulp sass",
    "build:sass:min": "gulp sass:min",
    "watch": "gulp watch:sass",
    "styleguide": "gulp styleguide",
    "sites": "gulp sass && concurrently \"lite-server -c bs-config.json\" \"gulp watch:sass\" "
  },
  "keywords": ["css", "sass", "web"],
  "author": "orotter",
  "license": "MIT",
  "style": "css/orotter-bootstrap.css",
  "main": "css/orotter-bootstrap.css",
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "browser-sync": "^2.18.5",
    "cheerio": "^0.22.0",
    "concurrently": "^3.1.0",
    "cssnano": "^3.9.1",
    "gulp": "^3.9.1",
    "gulp-if": "^2.0.2",
    "gulp-postcss": "^6.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.0.0",
    "lite-server": "^2.2.2",
    "node-sass": "^4.0.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-style-guide": "^0.13.0",
    "psg-theme-sassline": "github:sotayamashita/psg-theme-sassline"
  },
  "dependencies": {
    "normalize.css": "^5.0.0"
  }
}
